---
package_name: ExampleTweak
install_command: sbreload
Example:
    type: tweak
    targetios: 11.0
    archs:
        - arm64
        - arm64e
    logos_files:
        - Tweak.xm
ExamplePrefs:
    dir: ExampleTweakPrefs
    type: bundle
    targetios: 11.0
    archs:
        - arm64
        - arm64e
    files:
        - Prefs.m
    stage: # These two are required for PreferenceLoader
        - mkdir -p .dragon/_/Library/PreferenceLoader/Preferences/
        - cp entry.plist .dragon/_/Library/PreferenceLoader/Preferences/$name.plist